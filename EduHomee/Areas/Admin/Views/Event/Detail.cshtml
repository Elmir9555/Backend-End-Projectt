@using EduHomee.Models
@model Event

<form class="form-control">
    <div class="mb-3">
        <label class="form-label" style="font-size:34px;">Title</label>
        <div style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Title
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">Description</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Description
        </div>
    </div>

    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">Image</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Image
        </div>
    </div>
    <div class="mb-3">
        <label class="form-label" style="font-size:34px;">Location</label>
        <div style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Location
        </div>
    </div>
    <div class="mb-3">
        <label class="form-label" style="font-size:34px;">StartDate</label>
        <div style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.StartDate
        </div>
    </div>
    <div class="mb-3">
        <label class="form-label" style="font-size:34px;">EndDate</label>
        <div style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.EndDate
        </div>
    </div>
    <div class="mb-3">
        <label class="form-label" style="font-size:34px;">Date</label>
        <div style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Date
        </div>
    </div>
    <a asp-action="Index" class="btn btn-primary mt-3">Go back</a>
</form>





















@using EduHomee.Models
@model CourseFeatures

<form class="form-control">

    <div class="mb-3">
        <label class="form-label" style="font-size:34px;">CourseName</label>
        <div style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Course.Title
        </div>
    </div>
    <div class="mb-3">
        <label class="form-label" style="font-size:34px;">Assesments</label>
        <div style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Assesments
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">ClassDuration</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.ClassDuration
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">CourseFee</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.CourseFee
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">Duration</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Duration
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">Language</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Language
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">SkillLevel</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.SkillLevel
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">Start</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Start
        </div>
    </div>
    <div class="mt-3">
        <label class="form-label" style="font-size:34px;">Students</label>

        <div class="mt-1" style="width:100%;height:40px;border:1px dashed grey;padding-top:10px;padding-left:20px;">
            @Model.Students
        </div>
    </div>
   
    <a asp-action="Index" class="btn btn-primary mt-3">Go back</a>
</form>




















